<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Nav Properties"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/tabs"
                    maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <toplayer
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Image and Headline Settings"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true"
                    >
                        <items jcr:primaryType="nt:unstructured">
                        <column
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/container">
                            <items jcr:primaryType="nt:unstructured">
                                <imagepath
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Image Path"
                                        name="./imagepath"/>
                                <imagealt
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Image Alt Value"
                                    name="./imagealt"/>
                                <headline
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Headline"
                                        name="./headline"/>
                            </items>
                        </column>
                        </items>
                    </toplayer>
                    <cardsettings
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Cards Settings"
                            sling:resourceType="granite/ui/components/coral/foundation/container"
                            margin="{Boolean}true"
                    >
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <cardheader1
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldLabel="Card Header 1"
                                            name="./cardheader1"/>
                                    <cardcontent1
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldLabel="Card Text 1"
                                            name="./cardcontent1"/>
                                    <cardheader2
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldLabel="Card Header 2"
                                            name="./cardheader2"/>
                                    <cardcontent2
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldLabel="Card Text 2"
                                            name="./cardcontent2"/>
                                    <cardheader3
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldLabel="Card Header 3"
                                            name="./cardheader3"/>
                                    <cardcontent3
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldLabel="Card Text 3"
                                            name="./cardcontent3"/>
                                    <cardheader4
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldLabel="Card Header 4"
                                            name="./cardheader4"/>
                                    <cardcontent4
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldLabel="Card Text 4"
                                            name="./cardcontent4"/>
                                </items>
                            </column>
                        </items>
                    </cardsettings>
                    <tabsettings
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Nav 3 Settings"
                            sling:resourceType="granite/ui/components/coral/foundation/container"
                            margin="{Boolean}true"
                    >
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <tab1
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldLabel="Tab Name"
                                            name="./tab1"/>
                                    <tab2
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldLabel="Tab Name"
                                            name="./tab2"/>
                                    <tab3
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldLabel="Tab Name"
                                            name="./tab3"/>
                                    <tab4
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldLabel="Tab Name"
                                            name="./tab4"/>
                                </items>
                            </column>
                        </items>
                    </tabsettings>
                </items>
            </tabs>
        </items>

    </content>
</jcr:root>